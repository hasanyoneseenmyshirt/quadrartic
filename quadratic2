#include <iostream>
#include <cmath>

int main() {
  double a,b,c,root1,root2;
  std::cout <<"enter a: \n";
  std::cin >> a;
  std::cout <<"enter b: \n";
  std::cin >> b;
  std::cout <<"enter : \n";
  std::cin >> c;

  root1 = (-b + std::sqr(b * b - 4*a(c)))/(2*a);

  root2 = (-b - std::sqr(b * b - 4*a(c)))/(2*a);

  std::cout << "root 1: " << root1 << "\n";
  std::cout << "root 2: " << root2 <<"\n";
  return 0;
